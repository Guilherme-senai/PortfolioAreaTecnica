Segunda forma Normal
Drop Table JA
	 SELECT * FROM JA;
	 CREATE TABLE JA(--1
		CPF NUMERIC(9) NOT NULL,--Primaria
		NOME VARCHAR(30) NOT NULL,
		SOBRENOME VARCHAR(30)NOT NULL,
		DT_NASC DATE NOT NULL,
		DEFICIENCIA VARCHAR(3)NOT NULL,
		JA VARCHAR(50)NOT NULL,
		DT_FIM_JA DATE NOT NULL,
		RUA VARCHAR(50)NOT NULL,
		NR NUMERIC(5)NOT NULL,
		BAIRRO VARCHAR(50)NOT NULL,
		COMP VARCHAR(30),
		RG NUMERIC(15)NOT NULL,
		MOTIV_JA VARCHAR(9)NOT NULL,
		COD_SX NUMERIC(1)NOT NULL,--Estrangeira 1
		COD_GE NUMERIC(1)NOT NULL,--Estrangeira 2
		PAIS VARCHAR(100)NOT NULL,--Estrangeira 3
		ESTADO VARCHAR(100)NOT NULL,--Estrangeira 4
		CIDADE VARCHAR(100)NOT NULL,--Estrangeira 5
		COD_TE NUMERIC(1)NOT NULL,--Estrangeira 6
		COD_TS NUMERIC(1)NOT NULL,--Estrangeira 7

COD_AS NUMERIC(1)NOT NULL--Estrangeira 8
	)
	Drop Table JA_CUR
    SELECT * FROM JA_CUR;
	CREATE TABLE JA_CUR(--2
	CPF NUMERIC(9)NOT NULL,--Primaria/Estrangeira
	COD_AC VARCHAR(1)NOT NULL--Primaria/Estrangeira
	)
	Drop Table AC
	SELECT * FROM AC;
	CREATE TABLE AC(--3
	COD_AC NUMERIC(2)NOT NULL,--Primaria
	DESC_AC VARCHAR(100)NOT NULL
	)
	Drop Table SX
	SELECT * FROM SX;
	CREATE TABLE SX(--4
	COD_SX NUMERIC(1)NOT NULL,--Primaria
	DESC_SX VARCHAR(100)NOT NULL
	)
	Drop Table GE
	SELECT * FROM GE;
	CREATE TABLE GE(--5
	COD_GE NUMERIC(1)NOT NULL,--Primaria
	DESC_GE VARCHAR(100)NOT NULL
	)
	Drop Table TE
	SELECT * FROM TE;
	CREATE TABLE TE(--6
	COD_TE NUMERIC(1)NOT NULL,--Primaria
	DESC_TE VARCHAR(100)NOT NULL
	)
	Drop Table TS
	SELECT * FROM TS;
	CREATE TABLE TS(--7
	COD_TS NUMERIC(1)NOT NULL,--Primaria
	DESC_TS VARCHAR(100)NOT NULL
	)
	
    -- CHAVES PRIMARIAS
	ALTER TABLE JA ADD CONSTRAINT PK_CPF PRIMARY KEY (CPF);
	ALTER TABLE JA_CUR ADD CONSTRAINT PK_CUR_CPF PRIMARY KEY (CPF);
	ALTER TABLE AC ADD CONSTRAINT PK_AC  PRIMARY KEY (COD_AC);
	ALTER TABLE SX ADD CONSTRAINT PK_SX PRIMARY KEY (COD_SX);
	ALTER TABLE GE ADD CONSTRAINT PK_GE PRIMARY KEY (COD_GE);
	ALTER TABLE TE ADD CONSTRAINT PK_TE PRIMARY KEY (COD_TE);
	ALTER TABLE TS ADD CONSTRAINT PK_TS PRIMARY KEY (COD_TS);
	ALTER TABLE CIDADE ADD CONSTRAINT PK_CIDADE PRIMARY KEY (COD_CID);
	ALTER TABLE ESTADO ADD CONSTRAINT PK_EST PRIMARY KEY (COD_EST);
	ALTER TABLE PAIS ADD CONSTRAINT PK_PAIS PRIMARY KEY (COD_PAIS);
	
	-- CHAVE ESTRANGEIRA
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_SX
	FOREIGN KEY(COD_SX)
	REFERENCES SX(COD_SX)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_GE
	FOREIGN KEY(COD_GE)
	REFERENCES GE(COD_GE)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_AC
	FOREIGN KEY(COD_AC)
	REFERENCES AC(COD_AC)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_TE
	FOREIGN KEY(COD_TE)
	REFERENCES TE(COD_TE)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_TS
	FOREIGN KEY(COD_TS)
	REFERENCES TS(COD_TS)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_CIDADE
	FOREIGN KEY(COD_CID)
	REFERENCES CIDADE(COD_CID)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_ESTADO
	FOREIGN KEY(COD_EST)
	REFERENCES ESTADO(COD_EST)
	
	ALTER TABLE JA
	ADD CONSTRAINT FK_JA_PAIS
	FOREIGN KEY(COD_PAIS)
	REFERENCES PAIS(COD_PAIS)
	
	ALTER TABLE JA_CUR
	ADD CONSTRAINT FK_JA_CUR_CPF
	FOREIGN KEY(CPF)
	REFERENCES JA(CPF)
	
	ALTER TABLE JA_CUR
	ADD CONSTRAINT FK_JACUR_AC
	FOREIGN KEY(COD_AC)
	REFERENCES AC(COD_AC)
	
