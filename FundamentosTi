Comando para descobrir nomes
#!/bin/bash
cut -d: -f1 /etc/passwd

comando permisionalmeto
#!/bin/bash
var=$(ls -ld $1)

tamr=${var: 7 : 1}
tamw=${var: 8 : 1}
tamx=${var: 9: 1}
if [[$tamr == 'r' ]]
echo tem escrita
fi
if [[$tamw == 'w' ]]
echo tem leitura
fi
if [[$tamx == 'x' ]]
then
echo tem execução
fi

#!/bin/bash
echo "digite  seu ano de nascimento"
read ano_nascimento
echo " Escreva o ano atual :"
read ano_atual

idade=$((ano_atual - ano_nascimento))
if [$idade -le 15]
then
echo" você pode votar"
else
echo " você deve votar"
fi

